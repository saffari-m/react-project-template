{
  "name": "react-project-template",
  "version": "1.0.0",
  "description": "React project template",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "build:start": "cd dist && PORT=8080 npx serve",
    "start": "webpack serve --open --config webpack.dev.js",
    "start:live": "webpack serve --open --config webpack.dev.js --liveReload",
    "docker:build": "docker build . -t wp5-starter",
    "docker:run": "docker run -p 8080:8080 wp5-starter",
    "prepare": "husky install",
    "prettier": "prettier --check 'src/*.{js,jsx}' --write",
    "lint-staged": "lint-staged"
  },
  "keywords": [
    "react.js",
    "template",
    "webpack"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.18.0",
    "eslint-plugin-react": "^7.30.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.7.1",
    "style-loader": "^3.3.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,css}": [
      "eslint --fix",
      "prettier --write",
      "git add ."
    ]
  }
}
